{
  "name": "zero-trust-analytics",
  "version": "1.0.0",
  "description": "Privacy-focused, anonymous website analytics. Track visitors without compromising their privacy.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config tests/jest.config.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config tests/jest.config.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config tests/jest.config.js --coverage",
    "obfuscate": "npx javascript-obfuscator src/analytics.js --output static/js/analytics.js --compact true --control-flow-flattening true --dead-code-injection true --string-array true --string-array-encoding base64 --string-array-threshold 0.75",
    "build": "npm run obfuscate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsutter87/zero-trust-analytics.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jasonsutter87/zero-trust-analytics/issues"
  },
  "homepage": "https://github.com/jasonsutter87/zero-trust-analytics#readme",
  "devDependencies": {
    "javascript-obfuscator": "^5.0.1",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "resend": "^6.6.0"
  }
}
